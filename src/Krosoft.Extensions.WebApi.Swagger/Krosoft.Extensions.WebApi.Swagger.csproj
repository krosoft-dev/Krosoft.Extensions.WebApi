<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OpenApiGenerateDocuments>false</OpenApiGenerateDocuments>
		<PackageTags>Krosoft Extension Web Api AspNetCore swagger</PackageTags>
		<Description>Package pour faciliter l'utilisation de Swagger avec ASP.NET Core.</Description>
	</PropertyGroup> 	
	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
	</ItemGroup> 	
	<ItemGroup> 
		<PackageReference Include="Krosoft.Extensions.Core" Version="2025.5.5.1450" /> 
		<PackageReference Include="Swashbuckle.AspNetCore" Version="6.7.3" /> 
		<PackageReference Include="Swashbuckle.AspNetCore.Annotations" Version="6.7.3" />
		<PackageReference Include="Swashbuckle.AspNetCore.SwaggerGen" Version="6.7.3" /> 
	</ItemGroup> 	

</Project>

   
 